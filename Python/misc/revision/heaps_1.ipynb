   "source": [
    "# purpose of the heap is to get smallest/largest val quickly\n",
    "# basically a mod of the binary tree\n",
    "\n",
    "# min heap -> parent <= left_node\n",
    "#          -> parent <= right_node"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "id": "33cb8e48",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "[1, 10, 4]\n"
     ]
    }
   ],
   "source": [
    "import heapq\n",
    "\n",
    "h = []\n",
    "\n",
    "heapq.heappush(h, 10)\n",
    "heapq.heappush(h, 4)\n",
    "heapq.heappush(h, 1)\n",
    "\n",
    "print(h)"
   ]
  },
