   "source": [
    "def bubble_sort(arr):\n",
    "    n = len(arr)\n",
    "\n",
    "    for i in range(n-1):\n",
    "        for j in range(n-i-1):\n",
    "            if arr[j] > arr[j+1]:\n",
    "                arr[j], arr[j+1] = arr[j+1], arr[j]\n",
    "\n",
    "arr = [36, 22, 98, -121, 11, 2, 13, 1, 34, -10, -4]\n",
    "bubble_sort(arr=arr)\n",
    "print(arr)"
   ]
